{
  "private": true,
  "name": "amperio-partners",
  "version": "0.0.1",
  "author": "Amperio",
  "scripts": {
    "build": "craco build",
    "check-all": "yarn lint && yarn test && yarn build && yarn format:check && yarn typecheck && yarn commitlint && yarn stylelint",
    "commitlint": "commitlint --from=HEAD~1",
    "create-component": "plop --plopfile ./scripts/plop/create-component/create-component.js",
    "create-function": "plop --plopfile ./scripts/plop/create-function/create-function.js",
    "create-hook": "plop --plopfile ./scripts/plop/create-hook/create-hook.js",
    "create-transformator": "plop --plopfile ./scripts/plop/create-transformator/create-transformator.js",
    "docker:build": "docker build -t amperio-partners .",
    "docker:run": "docker run -p 8080:80 amperio-partners",
    "eject": "react-scripts eject",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --check",
    "gcb": "bash ./scripts/shell/git-create-branch.sh",
    "gcdwp": "bash ./scripts/shell/git-checkout-develop-with-pull.sh",
    "gpc": "bash ./scripts/shell/git-push-changes.sh",
    "grlc": "bash ./scripts/shell/git-reset-local-changes.sh",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "og": "bash ./scripts/shell/open-github.sh",
    "open:analyze": "yarn build && npx source-map-explorer 'build/static/js/*.js'",
    "open:coverage": "yarn test:coverage && open ./coverage/lcov-report/index.html",
    "prepare": "husky install",
    "reinstall-node-modules": "bash ./scripts/shell/reinstall-node-modules.sh",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "start": "craco start",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "yarn run stylelint --fix",
    "test": "REACT_APP_ENV=test jest",
    "test:ci": "REACT_APP_ENV=test jest --ci --coverage --forceExit",
    "test:coverage": "REACT_APP_ENV=test jest --coverage",
    "test:update-snapshot": "REACT_APP_ENV=test jest --updateSnapshot",
    "test:watch": "REACT_APP_ENV=test jest --watch",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json)\"",
    "typecheck": "tsc --noEmit -p . --pretty"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@fullcalendar/daygrid": "^5.10.0",
    "@fullcalendar/interaction": "^5.10.0",
    "@fullcalendar/react": "^5.10.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.54",
    "@mui/material": "^5.0.6",
    "@mui/x-data-grid": "^5.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.22.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "jwt-decode": "^3.1.2",
    "node-sass": "^6.0.1",
    "query-string": "^7.0.1",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-countup": "^6.1.1",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^17.0.2",
    "react-hook-form": "7.20.2",
    "react-intl": "^5.20.13",
    "react-query": "^3.27.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.1.0",
    "source-map-explorer": "^2.5.2",
    "ts-deep-pick": "^0.2.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@amperio/amperio-ui": "^1.42.4",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@craco/craco": "^6.3.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.45",
    "@types/react": "^17.0.0",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-detect-offline": "^2.4.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "8.1.0",
    "craco-alias": "^3.0.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^36.0.0",
    "husky": "^7.0.2",
    "inquirer-directory": "^2.2.0",
    "jest": "26.6.0",
    "jest-module-name-mapper": "^0.1.5",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^17.0.0",
    "lint-staged": "^11.2.0",
    "plop": "^2.7.4",
    "prettier": "^2.4.1",
    "prettier-package-json": "^2.6.0",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.5.2",
    "use-query-params": "^1.2.3",
    "yarnhook": ">=0.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test",
      "pre-commit": "lint-staged && yarn test",
      "post-checkout": "yarn yarnhook",
      "post-merge": "yarn yarnhook",
      "post-rewrite": "yarn yarnhook"
    }
  },
  "volta": {
    "node": "14.18.2",
    "yarn": "1.22.17"
  }
}
